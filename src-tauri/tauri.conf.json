{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Smart Cleaner",
  "version": "1.1.0",
  "identifier": "com.gefangshuai.smartcleaner",
  "plugins": {
    "updater": {
      "pubkey": "UPDATE_YOUR_PUBKEY_HERE",
      "endpoints": ["https://example.com/latest.json"]
    }
  },
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Smart Cleaner",
        "width": 1000,
        "height": 800,
        "minWidth": 800,
        "minHeight": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
